{
  "manifest_version": 3,
  "name": "Phishy",
  "version": "1.0",
  "description": "Find out if the site you're visiting is a phishing site ðŸŽ£",
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/phishy-16.png",
      "48": "icons/phishy-48.png",
      "128": "icons/phishy-128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content.js"]
    }
  ]
}
